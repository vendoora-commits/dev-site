{
  "name": "vendoora-mcp-ai-coding",
  "version": "1.0.0",
  "description": "Advanced AI-powered coding assistant with code generation, analysis, and optimization",
  "main": "dist/ai-coding-mcp.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/ai-coding-mcp.ts",
    "start": "node dist/ai-coding-mcp.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "ai-coding",
    "code-generation",
    "code-analysis",
    "code-optimization",
    "ai-assistant",
    "vendoora"
  ],
  "author": "Vendoora Team",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "chalk": "^5.0.0",
    "eslint": "^8.0.0",
    "fs-extra": "^11.0.0",
    "glob": "^10.0.0",
    "langchain": "^0.1.0",
    "openai": "^4.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
